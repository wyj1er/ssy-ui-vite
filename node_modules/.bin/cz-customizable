#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/git/web-monorepo/node_modules/.pnpm/cz-customizable@7.2.1/node_modules/cz-customizable/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/cz-customizable@7.2.1/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/git/web-monorepo/node_modules/.pnpm/cz-customizable@7.2.1/node_modules/cz-customizable/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/cz-customizable@7.2.1/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cz-customizable/standalone.js" "$@"
else
  exec node  "$basedir/../cz-customizable/standalone.js" "$@"
fi
