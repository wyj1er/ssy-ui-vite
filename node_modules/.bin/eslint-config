#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules/@antfu/eslint-config/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules/@antfu/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules/@antfu/eslint-config/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules/@antfu/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/@antfu+eslint-config@3.8.0_@eslint-react+eslint-plugin@1.14.2_eslint@9.13.0_jiti@1.21.6__type_f3cvr3aygd4e24b5ulqtadltzq/node_modules:/mnt/d/git/web-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
