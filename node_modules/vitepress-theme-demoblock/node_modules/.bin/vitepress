#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vitepress@1.4.1_@algolia+client-search@4.24.0_@types+node@22.5.4_@types+react@18.3.5_axios@1._t44nxhivxkk5qotcoc3xnhgq6q/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../../vitepress@1.4.1_@algolia+client-search@4.24.0_@types+node@22.5.4_@types+react@18.3.5_axios@1._t44nxhivxkk5qotcoc3xnhgq6q/node_modules/vitepress/bin/vitepress.js" "$@"
fi
